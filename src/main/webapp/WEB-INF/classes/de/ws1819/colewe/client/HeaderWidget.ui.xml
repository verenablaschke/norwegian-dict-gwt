<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.header {
		font-size: 1.3rem;
		margin: auto;
		margin-top: 30px;
		margin-bottom: 20px;
		text-align: center;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HTML ui:field="header" styleName="{style.header}"></g:HTML>
	</g:HTMLPanel>
</ui:UiBinder> 
