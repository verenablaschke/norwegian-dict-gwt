<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.space {
		margin-bottom: 2em;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:FocusPanel ui:field="table">
			<g:FlowPanel styleName="row" ui:field="flowPanel" />
		</g:FocusPanel>
		<br />
		<g:Button styleName="{style.space} btn btn-primary" ui:field="againButton">
			<i class="fa fa-undo"></i>
			Igjen
		</g:Button>
		<g:Button styleName="{style.space} btn btn-outline-primary" ui:field="downloadButton">
			<i class="fas fa-download"></i> 
			Last ned
		</g:Button>
		<br />
		<g:HTML ui:field="info" />
	</g:HTMLPanel>
</ui:UiBinder> 